# Teste com matplotlib (output gráfico)
import matplotlib.pyplot as plt

x = [1, 2, 3, 4, 5]
y = [n**2 for n in x]

plt.plot(x, y, marker="o")
plt.title("Quadrados de 1 a 5")
plt.xlabel("x")
plt.ylabel("y")
plt.show()

